doctype html
html
    head(lang='en')
        meta(charset='UTF-8')
        title
        link(rel='stylesheet', href='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.1/css/bootstrap.min.css')
        link(rel='stylesheet', href='../dist/botany.css')
    body
        ul.botany(class=theme)
            li List 1
                ul
            li List 2
                ul
            li.open List 3
                ul

        button.randomize(type="button") Add Random Nodes
        script(src='https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js')
        script(src='../dist/script.min.js')
        script.
            $(function () {
                $(".botany").botany();

                var counter = 0;
                $(".randomize").click(function () {
                    var lists = $(".botany ul");
                    var index = Math.floor(Math.random() * lists.length);
                    $(lists[index]).append("<li class='open'> Inserted " + counter++  + "<ul></ul></li>");
                });
            });